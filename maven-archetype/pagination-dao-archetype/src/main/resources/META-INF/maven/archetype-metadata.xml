<?xml version="1.0"?>

<archetype-descriptor name="pagination-dao-archetype">
	<fileSets>
		<fileSet filtered="true" packaged="true">
			<directory>src/main/java</directory>
			<includes>
				<include>**/*.java</include>
			</includes>
		</fileSet>
	</fileSets>
	<requiredProperties>
		<requiredProperty key="daoClassName">
			<defaultValue>EntityDAO</defaultValue>
		</requiredProperty>
	</requiredProperties>
</archetype-descriptor>
